{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch (only)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Getkon/bin/Debug/net9.0/Getkon.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Getkon",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Attach to Electron Renderer",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    },
    {
      "command": "npm run dev",
      // "name": "Run development server",
      "request": "launch",
      "type": "node-terminal",

      // "command": "npm run dev",
      // "request": "launch",
      // "type": "node-terminal",
      "name": "Frontend Launch (only)"
      // "cwd": "${workspaceFolder}/frontend/svelte",
    }
  ],
  "compounds": [
    {
      "name": "Launch All",
      "configurations": [".NET Launch (only)", "Frontend Launch (only)"]
    }
  ]
}
